{
  "meta": {
    "description": "Use this GOV.UK Prototype Kit plugin to add notification overlays to GOV.UK prototype kit.",
    "urls": {
      "documentation": "https://github.com/jrmedd/govuk-notificatons",
      "releaseNotes": "https://github.com/jrmedd/govuk-notificatons",
      "versionHistory": "https://npmjs.com/package/govuk-notifications?activeTab=versions"
    }
  },
  "pluginDependencies": [
    "govuk-prototype-kit"
  ],
  "nunjucksPaths": [
    "/"
  ],
  "nunjucksMacros": [
    {
      "macroName": "govukNotificationsPhone",
      "importFrom": "govukNotifications/components/phone.njk"
    },
    {
      "macroName": "govukNotificationsEmail",
      "importFrom": "govukNotifications/components/email.njk"
    }
  ],
  "sass": [
    "/sass/styles.scss"
  ],
  "scripts": [
    "/scripts/javascript.js"
  ],
  "nunjucksFilters": [
    "/lib/timeAndDate.js"
  ]
}